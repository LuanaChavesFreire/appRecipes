generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id       Int    @id @default(autoincrement())
  fullname String @db.VarChar(45)
  email    String @unique @db.VarChar(45)
  password String @db.VarChar(10)
}
